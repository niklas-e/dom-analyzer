{
    "manifest_version": 2,
    "name": "DOM Analyzer",
    "version": "1.0",
    "description": "Analyzes your page's DOM structure.",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.mozilla.org/*"
            ],
            "js": [
                "analyzer.js"
            ]
        }
    ]
}