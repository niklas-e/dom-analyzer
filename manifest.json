{
    "manifest_version": 2,
    "name": "DOM Analyzer",
    "version": "1.0",
    "description": "Analyzes your page's DOM structure.",
    "icons": {
        "48": "icons/border-48.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/border-48.png",
        "default_title": "DOM Analyzer",
        "default_popup": "popup.html"
    }
}